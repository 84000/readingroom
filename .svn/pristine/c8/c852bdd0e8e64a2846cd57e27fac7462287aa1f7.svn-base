<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:org.tbrc.readingroom.client">

	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>

	<g:HTMLPanel>
		<my:Header ui:field="header"/>
		<!-- DeckPanel for animated transitions between pages -->
		<g:DeckPanel ui:field="deckPanel" animationEnabled="true">
			<!-- Initial child panels - more Section panels can be added dynamically -->
			<!-- <my:Lobby ui:field="lob"/> -->
			<!-- <my:Section ui:field="section"/> -->
		</g:DeckPanel>
	</g:HTMLPanel>
</ui:UiBinder> 